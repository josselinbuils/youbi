{
  "name": "youbi",
  "version": "0.1.0",
  "main": "dist/node/main.js",
  "scripts": {
    "build": "yarn build:browser && yarn build:node",
    "build:browser": "rm -rf dist/browser && NODE_ENV=production webpack --config ./config/browser/webpack.config.js",
    "build:node": "rm -rf dist/node dist/shared && tsc -p ./config/node/tsconfig.json",
    "dev": "concurrently -n browser,node,electron 'yarn dev:browser' 'yarn dev:node' 'yarn dev:electron'",
    "dev:browser": "node ./scripts/dev.js",
    "dev:node": "yarn build:node -w",
    "dev:electron": "chokidar 'dist/node/**/*' -c 'electron .'",
    "lint": "eslint .",
    "start": "electron ."
  },
  "dependencies": {
    "@josselinbuils/utils": "^0.3.0",
    "ansicolor": "^1.1.93",
    "electron": "^11.0.1",
    "electron-store": "^6.0.1",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^9.0.1",
    "ipc-promise": "^0.1.4",
    "jimp": "^0.16.1",
    "music-metadata": "^7.5.0",
    "remove": "^0.1.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@josselinbuils/eslint-config-react": "^0.1.0",
    "@josselinbuils/hooks": "^0.2.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/classnames": "^2.2.11",
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/request-promise-native": "^1.0.17",
    "babel-loader": "^8.2.1",
    "chokidar-cli": "^2.1.0",
    "classnames": "^2.2.6",
    "color-thief-browser": "^2.0.2",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "dayjs": "^1.9.6",
    "eslint": "^7.14.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-alpha.14",
    "mini-css-extract-plugin": "^1.3.1",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-refresh": "^0.9.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.5.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.5"
  },
  "browserslist": [
    "chrome 87"
  ]
}
